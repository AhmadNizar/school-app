<!DOCTYPE html>
<html>
<head>
	<title></title>
    <% include ./partials/head %>
</head>
<body class='container'>
<main>
<header>
    <% include ./partials/header %>
</header>
        <div class="row">

                    <h1>Student</h1>
                    <table style="width:100%">
                        <tr>
                            <th>ID</th>                            
                        	<th>First Name</th>
                            <th>Last Name</th>
                            <th>Email</th>
                            <th>Action</th>
                            <th>Option</th>
                        </tr>
                        <% allStudent.forEach((temps) => { %>
                        <tr>
                            <td><%= temps.id%></td>
                            <td><%= temps.first_name%></td>
                            <td><%= temps.last_name%></td>
                            <td><%= temps.email%></td>
                            <td><a href="/student/addSubjectByStudent/<%= temps.id%>">Add Subject</a></td>
                            <td>
                                <a href="/student/deleteStudent/<%= temps.id%>">Delete</a>
                                <a href="/student/edit/<%= temps.id%>">Edit</a> 
                            </td>
                        </tr>
                        <% }); %>
                    </table>
        </div>
<footer>
    <% include ./partials/footer %>
</footer>
    </main>
</body>
</html>