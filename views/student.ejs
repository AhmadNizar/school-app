<!DOCTYPE html>
<html>
<head>
	<title></title>
</head>
<body>
<main>
        <div class="row">
       
                    <h1>Student</h1>
                    <h2><a href="/student/add">Add Student</a></h2>
                    <ul>
                        <li><a href="/teacher">Teacher</a></li>
                        <li><a href="/subject">Subject</a></li>
                        <li><a href="/student">Student</a></li>
                    </ul>
                    <table style="width:100%">
                        <tr>
                            <th>ID</th>                            
                        	<th>First Name</th>
                            <th>Last Name</th>
                            <th>Email</th>
                            <th>Action</th>
                        </tr>
                        <% allStudent.forEach((temps) => { %>
                        <tr>
                            <td><%= temps.id%></td>
                            <td><%= temps.first_name%></td>
                            <td><%= temps.last_name%></td>
                            <td><%= temps.email%></td>
                            <td>
                                <a href="/deleteStudent/<%= temps.id%>">Delete</a>
                                <a href="/edit/<%= temps.id%>">Edit</a> 
                            </td>
                        </tr>
                        <% }); %>
                    </table>
                </div>
            </div>
            <div>
    </main>
</body>
</html>